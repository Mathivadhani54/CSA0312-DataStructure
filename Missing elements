#include <stdio.h>
int findMissing(int A[], int N) 
{
int total = N * (N + 1) / 2;
int sum = 0;
for (int i = 0; i < N - 1; i++) 
{
sum += A[i]:
 }
 return total - sum;
}
int main() 
{
 int A1[] = {1, 2, 3, 5};
 int N1 = 5;
 printf("Missing element: %d\n", findMissing(A1, N1));
 int A2[] = {6, 1, 2, 8, 3, 4, 7, 10, 5};
 int N2 = 10;
 printf("Missing element: %d\n", findMissing(A2, N2));
 return 0;
}
